<div class="container">
    <div class="section1 my-2">
        <div class="row">
            <div class="col-md-6">
                <div class="bd-example slider1">
                    <div id="carouselExampleCaptions" class="carousel slide" data-ride="carousel">
                        <ol class="carousel-indicators">
                            <li data-target="#carouselExampleCaptions" data-slide-to="0" class="active"></li>
                            <li data-target="#carouselExampleCaptions" data-slide-to="1"></li>
                            <li data-target="#carouselExampleCaptions" data-slide-to="2"></li>
                            <li data-target="#carouselExampleCaptions" data-slide-to="3"></li>
                            <li data-target="#carouselExampleCaptions" data-slide-to="4"></li>
                        </ol>
                        <div class="carousel-inner">
                            {{#each topnews.slidernews}}
                            <div class="carousel-item">
                                <img src= {{this.urlToImage}} class="d-block w-100" alt="First Slide">
                                <div class="carousel-caption d-block d-md-block">
                                    <h5><a href="/detailnews/?url={{this.url}}">{{this.title}}</a></h5>
                                    <p><a href="#"></a></p>
                                </div>
                            </div>
                            {{/each}}
                        </div>
                        <a class="carousel-control-prev" href="#carouselExampleCaptions" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleCaptions" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <h1 class="topnews">TOP NEWS STORIES</h1>                
                <div class="list-group slider1-aside">
                    {{#each topnews.asideslidernews}}
                    <a href="/detailnews/?url={{this.url}}" class="list-group-item list-group-item-action ">
                        <div class="d-flex w-100 justify-content-between">
                            <h5 class="mb-1">{{this.title}}</h5>                            
                        </div>
                        <!-- <p class="mb-1">{{this.description}}</p> -->
                        <small>Source: {{this.source.name}}</small>
                    </a>
                    {{/each}}
                </div>
            </div>            
        </div>        
    </div>

    <!-- section 2 Starts -->
    <div class="section2 bg-fgray my-2">
        <div class="row">
            <div class="col-md-6 s2media">
                <ul class="list-unstyled">
                    {{#each topnews.bellowslidernews}}
                    <a href="/detailnews/?url={{this.url}}">
                    <li class="media my-1 pb-1">
                        <img src={{this.urlToImage}} class="align-self-center mr-3" alt="sorry!.. No image">
                        <div class="media-body text-align-justify">
                            <h5 class="mt-0 mb-1"></h5>
                            {{this.title}}
                        </div>
                    </li>
                    </a>
                    {{/each}}
                </ul>
            </div>
            <div class="col-md-6 s2card">
                <div class="card-group">
                    {{#each topnews.s2VCnews}}
                    <div class="card">
                        <img src={{this.urlToImage}} class="card-img-top" alt="...">
                        <div class="card-body">
                            <a href="/detailnews/?url={{this.url}}">
                                <h5 class="card-title"></h5>
                                <p class="card-text">{{this.title}}</p>
                                <p class="card-text"><small class="text-muted">{{this.publishedAt}}</small></p>
                            </a>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
    <!-- section 2 Ends -->

    <!-- section 3 starts -->
