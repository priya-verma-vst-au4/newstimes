<div class="container">  
    {{#if news}} 
    {{#each news}}
    <div class="row no-gutters category-border my-2">
        <div class="col-md-4">
            <img src={{this.urlToImage}} class="card-img" alt="...">
        </div>
        <div class="col-md-8">
            <div class="card-body">
                <a href="/cdetailnews/?url={{this.url}}"><h5 class="card-title">{{this.title}}</h5></a>
                <p class="card-text">{{this.description}}</p>
                <p class="card-text"><small class="text-muted">{{this.author}}</small></p>
                <p class="card-text"><small class="text-muted">{{this.publishedAt}}</small></p>
            </div>
        </div>
  </div>
  {{/each}}
  {{else}}
    <div class="col-md-12">
        <h1 class="topnews my-2">Sorry Nothing Found !.......</h1>
    </div>  
  {{/if}}
</div>